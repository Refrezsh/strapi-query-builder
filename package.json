{
  "name": "@refresh/strapi-query-builder",
  "version": "0.0.1",
  "description": "Utility for creating Strapi queries in declarative vay",
  "main": "./lib/cjs/index.js",
  "types": "./lib/cjs/index.d.ts",
  "module": "./lib/esm/index.js",
  "scripts": {
    "clean": "rimraf lib",
    "build:esm": "tsc -p ./tsconfig.esm.json",
    "build:cjs": "tsc -p ./tsconfig.cjs.json",
    "build": "npm run clean && npm run build:esm && npm run build:cjs",
    "prepack": "npm run build",
    "test": "jest",
    "speed": "node tests/performanceTest.js",
    "coverage": "jest --coverage"
  },
  "keywords": [
    "strapi",
    "utils",
    "utilities"
  ],
  "author": "Igor Badanyuk",
  "license": "MIT",
  "devDependencies": {
    "@jest/globals": "^29.3.1",
    "@types/jest": "^29.2.3",
    "@types/lodash": "^4.14.190",
    "jest": "^29.3.1",
    "lodash": "^4.17.21",
    "prettier": "^2.8.0",
    "rimraf": "^4.1.2",
    "ts-jest": "^29.0.3",
    "typescript": "^4.9.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JedWatson/classnames.git"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./lib/esm/types/index.d.ts",
        "default": "./lib/esm/index.mjs"
      },
      "require": {
        "types": "./lib/cjs/types/index.d.ts",
        "default": "./lib/cjs/index.js"
      }
    }
  },
  "files": [
    "lib/**/*"
  ]
}
